<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <filter>
        <filter-name>struts2</filter-name>
        <filter-class>org.apache.struts2.dispatcher.ng.filter.StrutsPrepareAndExecuteFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>struts2</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <servlet>
        <servlet-name>ListarBasesServlet</servlet-name>
        <servlet-class>br.gov.rn.emater.ListarBasesServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ResultadoServlet</servlet-name>
        <servlet-class>br.gov.rn.emater.ResultadoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>getImagemServlet</servlet-name>
        <servlet-class>br.gov.rn.emater.getImagemServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ListarBasesServlet</servlet-name>
        <url-pattern>/ListarBasesServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ResultadoServlet</servlet-name>
        <url-pattern>/ResultadoServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>getImagemServlet</servlet-name>
        <url-pattern>/getImagemServlet/*</url-pattern>
    </servlet-mapping>

    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
